<?xml version="1.0"?>
<config>
    <modules>
        <Ditosas_FedexCourrier>
            <version>1.0.0</version>
        </Ditosas_FedexCourrier>
    </modules>
    <global>
        <models>
            <ditosas_fedexcourrier>
                <class>Ditosas_FedexCourrier_Model</class>
                <resourceModel>ditosas_fedexcourrier_resource</resourceModel>
            </ditosas_fedexcourrier>
            <ditosas_fedexcourrier_resource>
                <class>Ditosas_FedexCourrier_Model_Resource</class>
                <entities>
                    <costos>
                        <table>corpaul_courrier_costos</table>
                    </costos>
                </entities>
            </ditosas_fedexcourrier_resource>
        </models>
        <helpers>
            <ditosas_fedexcourrier>
                <class>Ditosas_FedexCourrier_Helper</class>
            </ditosas_fedexcourrier>
        </helpers>
        <resources>
            <ditosas_fedexcourrier_setup>
                <setup>
                    <module>Ditosas_FedexCourrier</module>
                </setup>
            </ditosas_fedexcourrier_setup>
        </resources>
    </global>
    <default>
        <carriers>
            <ditosas_fedexcourrier>
                <active>1</active>
                <sallowspecific>1</sallowspecific>
                <model>ditosas_fedexcourrier/carrier</model>
                <name>FedEX</name>
                <price>25000</price>
                <title>FedEX</title>
            </ditosas_fedexcourrier>
        </carriers>
    </default>
</config>